<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>Zipkin</id>
		<title>Zipkin client</title>
		<version>$version$</version>
		<authors>hamilton verissimo (aka hammett)</authors>
		<owners>d-collab</owners>
		<licenseUrl>https://raw.githubusercontent.com/d-collab/zipkin.net/master/LICENSE</licenseUrl>
		<projectUrl>https://github.com/d-collab/zipkin.net</projectUrl>
		<!-- <iconUrl>http://docs.castleproject.org/GetFile.aspx?File=/castleLogo100px.png</iconUrl> -->
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Zipkin client for .net</description>
		<summary>If you have a zipkin server running, this client can connect to it using Rest or Scribe, Json or Thrift.</summary>
		<copyright>Copyright 2017 d-collab </copyright>
		<tags>zipkin distributed tracing</tags>

		<dependencies>
			<dependency id="NETStandard.Library" version="1.6.1" />
			<dependency id="System.Buffers" version="4.3.0" />
		</dependencies>

		<frameworkAssemblies>
			<frameworkAssembly assemblyName="System.Core" targetFramework="net461" />
		</frameworkAssemblies>
	</metadata>

	<files>
		<file src="LICENSE.txt" />
		
		<file src="src\Zipkin\bin\Release\net461\Zipkin.dll" target="lib/net461" />
		<file src="src\Zipkin\bin\Release\net461\Zipkin.pdb" target="lib/net461" />
		<file src="src\Zipkin\bin\Release\netstandard1.3\Zipkin.xml" target="lib/net461" />

		<file src="src\Zipkin\bin\Release\netstandard1.3\Zipkin.dll" target="lib/netstandard1.3" />
		<file src="src\Zipkin\bin\Release\netstandard1.3\Zipkin.pdb" target="lib/netstandard1.3" />
		<file src="src\Zipkin\bin\Release\netstandard1.3\Zipkin.xml" target="lib/netstandard1.3" />
	</files>

</package>